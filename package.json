{
  "name": "mtg-investment-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cleanup": "node scripts/maintenance/cleanup.js",
    "cleanup:dry": "node scripts/maintenance/cleanup.js --dry-run --verbose",
    "cleanup:agent": "node scripts/maintenance/workflow.js complete",
    "cleanup:final": "node scripts/maintenance/workflow.js final",
    "workflow:complete": "node scripts/maintenance/workflow.js complete",
    "workflow:final": "node scripts/maintenance/workflow.js final",
    "db:migrate": "node scripts/database/migrate-to-database.js",
    "db:migrate:dry": "node scripts/database/migrate-to-database.js --dry-run --verbose",
    "db:init": "node scripts/database/migrate-to-database.js",
    "db:info": "node -e \"const db = require('./src/lib/database'); db.default.Maintenance.getDatabaseInfo().then(console.log)\"",
    "db:vacuum": "node -e \"const db = require('./src/lib/database'); db.default.Maintenance.vacuum().then(() => console.log('Database vacuum completed'))\"",
    "db:check": "node scripts/database/database-config.js --check",
    "db:backup": "node scripts/db-backup.js",
    "db:postgres:setup": "node scripts/database/database-config.js --postgres-setup",
    "db:postgres:test": "node scripts/database/database-config.js --test-postgres",
    "db:validate": "node scripts/database/validate-database.js",
    "env:init": "node scripts/development/setup-env.js init",
    "env:validate": "node scripts/development/setup-env.js validate",
    "env:secret": "node scripts/development/setup-env.js generate-secret",
    "env:status": "node scripts/development/setup-env.js status",
    "test": "jest --config=config/jest.config.js",
    "test:unit": "jest --config=config/jest.config.js src/test/unit",
    "test:integration": "jest --config=config/jest.config.js src/test/integration",
    "test:e2e": "playwright test --config=config/playwright.config.ts",
    "test:coverage": "jest --config=config/jest.config.js --coverage",
    "test:watch": "jest --config=config/jest.config.js --watch",
    "test:ci": "jest --config=config/jest.config.js --ci --coverage --watchAll=false",
    "test:debug": "jest --config=config/jest.config.js --detectOpenHandles --forceExit",
    "lint:test": "eslint --config=config/eslint.config.mjs \"src/**/*.test.{ts,tsx}\" \"src/**/*.spec.{ts,tsx}\""
  },
  "dependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/papaparse": "^5.3.16",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "9.4.0",
    "critters": "^0.0.25",
    "dotenv": "^17.2.1",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.400.0",
    "next": "^13.4.19",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "sqlite3": "^5.1.7",
    "stream-json": "^1.9.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "postcss": "^8.4.0",
    "supertest": "^7.1.4"
  }
}
