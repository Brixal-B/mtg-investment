{
  "name": "mtg-investment-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cleanup": "node cleanup.js",
    "cleanup:dry": "node cleanup.js --dry-run --verbose",
    "cleanup:agent": "node workflow.js complete",
    "cleanup:final": "node workflow.js final",
    "workflow:complete": "node workflow.js complete",
    "workflow:final": "node workflow.js final",
    "db:migrate": "node migrate-to-database.js",
    "db:migrate:dry": "node migrate-to-database.js --dry-run --verbose",
    "db:init": "node migrate-to-database.js",
    "db:info": "node -e \"const db = require('./src/lib/database'); db.default.Maintenance.getDatabaseInfo().then(console.log)\"",
    "db:vacuum": "node -e \"const db = require('./src/lib/database'); db.default.Maintenance.vacuum().then(() => console.log('Database vacuum completed'))\""
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/sqlite3": "^5.1.0",
    "next": "15.4.6",
    "node-fetch": "^2.7.0",
    "papaparse": "^5.5.3",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sqlite3": "^5.1.7",
    "stream-json": "^1.9.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "glob": "^11.0.3",
    "postcss": "^8.5.6",
    "typescript": "^5"
  }
}
